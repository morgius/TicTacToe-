@model IEnumerable<TicTacToe.Models.Row>

<table>
    @if (Model != null && Model.Count() > 0)
    {
        foreach (var row in Model)
        {
            <tr id="r_@row.Id">
                @if (row.Colums != null && row.Colums.Count > 0)
                {
                    @foreach (var cell in row.Colums)
                    {
                        <td id="c_@cell.Id"></td>
                    }
                }

            </tr>
        }
    }
</table>
